static bool base_effect(effect_params_t* params) {
    RGB rgb;
    for(uint8_t i = 0; i < 56; i++) {
        rgb = hsv_to_rgb(base_hsv[i]);
        rgb_matrix_set_color(i, rgb.r, rgb.g, rgb.b);
    }
    return false;
}

